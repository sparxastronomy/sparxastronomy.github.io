---
import Layout from '../../layouts/Page.astro';
const seo = {
	title: 'Captures | Sparxastronomy',
	description: 'Captures by Bipradeep',
};
---

<Layout {seo}>
    <div class="relative h-[65vh] mx-auto my-8 md:w-3/4 align-middle items-center justify-center">
        <div class="text-center mt-10">
            <h2 class="font-emphasis heading-2">
                My captures
            </h2>
            <p class="text-2xl tracking-wide">
                Page under construction 🚧
            </p>
            <p class="text-2xl  tracking-wide">
                Please check back later ⏳
            </p>
        </div>
    </div>
</Layout>